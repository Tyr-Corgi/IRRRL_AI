{
  "ConnectionStrings": {
    "DefaultConnection": "Data Source=IRRRL.db"
  },
  "AzureOpenAI": {
    "Endpoint": "https://your-resource.openai.azure.com/",
    "ApiKey": "YOUR_API_KEY_HERE",
    "ModelName": "gpt-4",
    "MaxTokens": 2000,
    "Temperature": 0.7
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.Hosting.Lifetime": "Information"
      }
    }
  },
  "AllowedHosts": "*",
  "DocumentStorage": {
    "BasePath": "Documents",
    "MaxFileSizeMB": 10
  },
  "VASettings": {
    "MaxRecoupmentPeriodMonths": 36,
    "MinInterestRateReduction": 0.5,
    "DefaultFundingFeePercentage": 0.5
  }
}

